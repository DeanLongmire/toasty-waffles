<div class="tw-header-div">
  <div class="tw-grouped-buttons">
    <button
      {{on "click" (fn this.routeHeader "index")}}
      type="button"
      class="tw-button-no-border"
    >
      Toasty Waffles
    </button>
    {{#each this.seasons as |season|}}
      <button
        class="tw-button-no-border"
        type="button"
        {{on "click" (fn this.routeSeason season)}}
      >
        {{season.seasonRange}}
      </button>
    {{/each}}
  </div>
  <button
    class="tw-button-no-border"
    type="button"
    {{on "click" (fn this.routeHeader "league")}}
  >
    League
  </button>
  <button
    class="tw-button-no-border"
    type="button"
    {{on "click" (fn this.routeHeader "teams")}}
  >
    Teams
  </button>
</div>
<hr />