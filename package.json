  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@ember-data/adapter": "^5.7.0",
    "@ember-data/model": "^5.7.0",
    "@ember-data/serializer": "^5.7.0",
